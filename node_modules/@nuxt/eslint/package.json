{
  "name": "@nuxt/eslint",
  "type": "module",
  "version": "1.7.1",
  "description": "Generate ESLint config from current Nuxt settings",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/eslint.git",
    "directory": "packages/module"
  },
  "exports": {
    ".": "./dist/module.mjs"
  },
  "main": "./dist/module.mjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "eslint": "^9.0.0",
    "eslint-webpack-plugin": "^4.1.0",
    "vite-plugin-eslint2": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-webpack-plugin": {
      "optional": true
    },
    "vite-plugin-eslint2": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@nuxt/devtools-kit": "^2.6.2",
    "@nuxt/kit": "^4.0.1",
    "chokidar": "^4.0.3",
    "eslint-flat-config-utils": "^2.1.0",
    "eslint-typegen": "^2.2.1",
    "find-up": "^7.0.0",
    "get-port-please": "^3.2.0",
    "mlly": "^1.7.4",
    "pathe": "^2.0.3",
    "unimport": "^5.2.0",
    "@nuxt/eslint-config": "1.7.1",
    "@nuxt/eslint-plugin": "1.7.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.0.1",
    "@typescript-eslint/scope-manager": "^8.38.0",
    "eslint-webpack-plugin": "^5.0.2",
    "nuxt": "^4.0.1",
    "vite-plugin-eslint2": "^5.0.4"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "lint:play": "cd playground && eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  }
}